{
    "name": "maxpou/php-DP",
    "description": "Some Design Pattern implemented in PHP",
    "authors": [
        {
            "name": "Insan Wijaya",
            "email": "insan.career@gmail.com"
        }
    ],
    "require": {
        "symfony/var-dumper": "^3.1",
        "phpunit/phpunit": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "State\\":      ["DP/State_DP/Example/src",      "DP/State_DP/Example/tests"],
            "DTO\\":        ["DP/DTO_DP/Example/src",        "DP/DTO_DP/Example/tests"],
            "Strategy\\":   ["DP/Strategy_DP/Example/src",   "DP/Strategy_DP/Example/tests"],
            "Observer\\":   ["DP/Observer_DP/Example/src",   "DP/Observer_DP/Example/tests"],
            "NullObject\\": ["DP/NullObject_DP/Example/src", "DP/NullObject_DP/Example/tests"]
        }
    },
    "scripts": {
        "test": ".\\vendor\\bin\\phpunit",
        "test-dox": ".\\vendor\\bin\\phpunit --testdox",
        "test-coverage": ".\\vendor\\bin\\phpunit --coverage-text",
        "test-coverage-html": ".\\vendor\\bin\\phpunit --coverage-html=./reports/phpunit"
    }
}
